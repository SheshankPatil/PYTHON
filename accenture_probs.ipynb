    "v=int(input(\"enter number of vehicles\"))\n",
    "w =int(input('enter number of wheels in total'))\n",
    "\n",
    "def numberofdiffvehicles(v,w):\n",
    "    tw=((v*4)-w)/2\n",
    "    fw=v-tw\n",
    "\n",
    "    return tw,fw\n",
    "print(v,w)\n",
    "print(numberofdiffvehicles(v,w))"
   ]
